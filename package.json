{
  "name": "railview",
  "version": "0.0.5",
  "description": "RailView Frontend",
  "main": "index.js",
  "author": "Max Gorbachev <maxgorbs@gmail.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": "^8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Dan-Amaral/RailView-Project.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "build": "yarn && cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "lint": "eslint .",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test -u"
    }
  },
  "lint-staged": {
    "*.{js,css}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "automock": false,
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy",
      "^client(.*)$": "<rootDir>/client$1"
    },
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>config/jest/setup.js",
    "collectCoverageFrom": [
      "**/client/**/*.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "downloadjs": "^1.4.7",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "react": "^16.4.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.13",
    "webpack": "^4.21.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.2.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "browserslist": "^4.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.83.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.5",
    "postcss-calc": "^7.0.0",
    "postcss-import": "^12.0.0",
