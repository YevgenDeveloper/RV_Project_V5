    "prettier-stylelint": "^0.4.2",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-url-loader": "^2.3.2",
